{
    "name": "drodata/yii2-utility",

    "type": "yii2-extension",

    "description": "This extension includes some utilities that I used frequently.",
    "keywords": ["yii2", "helper"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/drodata/yii2-utility/issues",
        "source": "https://github.com/drodata/yii2-utility"
    },
    "authors": [
        {
            "name": "drodata",
            "email": "drodata@foxmail.com"
        }
    ],

    "require": {
        "yiisoft/yii2": "*",
        "fortawesome/font-awesome": "*",
		"bower-asset/bootstrap": "3.3.* | 3.2.* | 3.1.*",
        "npm-asset/bootstrap-tagsinput": "0.7.1"
    },

    "autoload": {
        "psr-4": {
            "drodata\\": ""
        }
    },
	"extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
